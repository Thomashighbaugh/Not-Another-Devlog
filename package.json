{
  "name": "gatsby-starter-blog",
  "private": true,
  "description": "A starter for a paginated blog powered by Gatsby and Markdown",
  "version": "1.1.0",
  "author": "Nicky Meuleman",
  "bugs": {
    "url": "https://github.com/NickyMeuleman/gatsby-paginated-blog/issues"
  },
  "dependencies": {
    "gatsby": "2.31.1",
    "gatsby-image": "2.10.0",
    "gatsby-plugin-feed": "2.12.0",
    "gatsby-plugin-google-analytics": "2.10.0",
    "gatsby-plugin-manifest": "2.11.0",
    "gatsby-plugin-offline": "3.9.0",
    "gatsby-plugin-react-helmet": "3.9.0",
    "gatsby-plugin-sass": "2.8.0",
    "gatsby-plugin-sharp": "2.13.2",
    "gatsby-remark-images": "3.10.0",
    "gatsby-remark-prismjs": "3.12.0",
    "gatsby-source-filesystem": "2.10.0",
    "gatsby-transformer-remark": "2.15.0",
    "gatsby-transformer-sharp": "2.11.0",
    "lodash": "4.17.20",
    "node-sass": "4.14.1",
    "prismjs": "1.23.0",
    "prop-types": "15.7.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-helmet": "5.2.1"
  },
  "devDependencies": {
    "eslint": "7.25.0",
    "eslint-plugin-react": "7.22.0",
    "prettier": "1.19.1"
  },
  "homepage": "https://github.com/NickyMeuleman/gatsby-paginated-blog#readme",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NickyMeuleman/gatsby-paginated-blog.git"
  },
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "develop": "gatsby develop",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'src/**/*.md'",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix gatsby-node.js",
    "deploy": "gh-pages -d public"
  }
}
